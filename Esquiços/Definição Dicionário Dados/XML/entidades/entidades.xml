<entities>

	<entity name="Cliente">
		<primaryKey name="pk_Cliente">
			<attribute name="id" type="xs:int" visibility="private"/>
		</primaryKey>
		<attribute name="nome" type="xs:string" visibility="private"/>
	</entity>
	
	<entity name="Fornecedor">
		<primaryKey name="pk_Forn">
			<attribute name="id" type="xs:int" visibility="private"/>
		</primaryKey>
		<attribute name="nome" type="xs:string" visibility="private"/>
	</entity>
	
	<entity name="Produto">
		<primaryKey name="pk_Prod">
			<attribute name="id" type="xs:int" visibility="private"/>
		</primaryKey>
		<attribute name="desc" type="xs:string" visibility="private"/>
		<relation name="codForn" srcEnt="Fornecedor" minOccurs="0" maxOccurs="unbounded"/>
	</entity>
	
	<entity name="Venda">
		<primaryKey name="pk_Venda">
			<attribute name="id" type="xs:int" visibility="private"/>
		</primaryKey>
		<relation name="codCli" srcEnt="Cliente" minOccurs="1" maxOccurs="1"/>
		<relation name="codProd" srcEnt="Produto" minOccurs="1" maxOccurs="unbounded"/>
	</entity>
	
	<entity name="User">
		<primaryKey name="pk_User">
			<attribute name="id" type="xs:int" visibility="private"/>
		</primaryKey>
		<relation name="roleId" srcEnt="Role" minOccurs="1" maxOccurs="unbounded"/>
	</entity>

	<entity name="Role">
		<primaryKey name="pk_Role">
			<attribute name="id" type="xs:int" visibility="private"/>
		</primaryKey>
		<relation name="permissionId" srcent="Permission" minOccurs="1" maxOccurs="unbounded"/>
	</entity>
	
	<entity name="Permission">
		<primaryKey name="pk_Name">
			<attribute name="name" type="xs:string" visibility="private"/>
		</primaryKey>
	</entity>
	
</entities>
