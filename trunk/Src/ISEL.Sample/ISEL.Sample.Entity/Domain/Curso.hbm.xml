<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="ISEL.Sample.Entity.Curso, ISEL.Sample.Entity" table="Curso" lazy="false" abstract="false">
		<id name="ID" column="Id">
			<generator class="identity" />
		</id>
		<property name="Nome" column="Nome" not-null="false" />
	
		<bag name="_Turma" access="field" table="Turma" inverse="true">
			<key column="CursoId" />
			<one-to-many class="ISEL.Sample.Entity.Turma, ISEL.Sample.Entity" />
		</bag>
	</class>
</hibernate-mapping>