<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="ISEL.Sample.Entity.Docente, ISEL.Sample.Entity" table="Docente" lazy="false" abstract="false">
		<id name="ID" column="Id">
			<generator class="identity" />
		</id>
		<property name="Nome" column="Nome" not-null="false" />
		<property name="Numero" column="Numero" not-null="false" />
	
		<bag name="_Lecciona" access="field" table="Calendario" inverse="true">
			<key column="DocenteId" />
			<one-to-many class="ISEL.Sample.Entity.Calendario, ISEL.Sample.Entity" />
		</bag>
	</class>
</hibernate-mapping>