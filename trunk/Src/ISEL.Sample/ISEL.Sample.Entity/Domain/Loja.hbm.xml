<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="ISEL.Sample.Entity.Loja, ISEL.Sample.Entity" table="Loja" lazy="false" abstract="false">
		<id name="ID" column="Id">
			<generator class="identity" />
		</id>
		<property name="Nome" column="Nome" not-null="false" />
	
		<bag name="_Albuns" access="field" table="LojaAlbum" inverse="true">
			<key column="LojaId" />
			<one-to-many class="ISEL.Sample.Entity.LojaAlbum, ISEL.Sample.Entity" />
		</bag>
	</class>
</hibernate-mapping>