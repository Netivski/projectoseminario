<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="FutureView.ECom.Entity.RuntimeAuthorizationUser, FutureView.ECom.Entity" table="RuntimeAuthorizationUser" lazy="false" abstract="false">
		<id name="ID" column="Id">
			<generator class="identity" />
		</id>
		<property name="UserName" column="UserName" not-null="false" />
	
		<many-to-one name="RuntimeAuthorization" class="FutureView.ECom.Entity.RuntimeAuthorization, FutureView.ECom.Entity" column="RuntimeAuthorizationId" cascade="all" />
	</class>
</hibernate-mapping>